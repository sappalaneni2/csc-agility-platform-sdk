   <xsd:complexType name="AccessList">
   
      <xsd:annotation>
         <xsd:appinfo>
            <annox:annotate target="class">
               <jxml:XmlRootElement>
                  <jxml:name>AccessList</jxml:name>
               </jxml:XmlRootElement>
               <a:ApiModel comment = "List of Access objects.  Protocols are part of the AccessList resource.  Access lists are deprecated as of Agility Platform version 7.2.  Existing access lists should have been migrated to NetworkACL Policies during the upgrade.  If an existing import file contains access lists, they will be converted to policies during the import process.  This resource is retained in order to check an existing system for access lists." 
                           description = "" 
                           externalDocLink = "" />
            </annox:annotate>
         </xsd:appinfo>
      </xsd:annotation>

      <xsd:complexContent>
         <xsd:extension base="agility:Item">
            <xsd:sequence>
               <xsd:element name="direction" type="agility:AccessListDirection" minOccurs="0">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <annox:annotate target="field">
                           <a:ApiField comment="Access list Direction object.  This is optional, but Direction must then be specified later with an update." />
                        </annox:annotate>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
               
               <xsd:element name="protocols" type="agility:Protocol" minOccurs="0" maxOccurs="unbounded">
                  <xsd:annotation>
                     <xsd:appinfo>
                        <annox:annotate target="field">
                           <a:ApiField comment="List of Protocol objects.  This is optional, but Protocols must be specified later with an update." />
                        </annox:annotate>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
  
